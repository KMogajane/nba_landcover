	keywords = {South Africa, Air quality, Environmental management policies, Eskom's environmental research programme, Mpumalanga Highveld region},
	pages = {109--114},
	file = {Full Text PDF:C\:\\Users\\K.Mogajane\\Zotero\\storage\\8XPCZQZF\\Rorich - 1998 - Air quality in the Mpumalanga Highveld region, Sou.pdf:application/pdf},
}

